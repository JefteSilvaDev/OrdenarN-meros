import javax.swing.JOptionPane;
 
public class desafioOrdem {
 

    public static void main(String[] args) { 
         
 
                int num1;
                int num2;
                int num3;
                int auxiliar;
 
                num1 = Integer.parseInt(JOptionPane.showInputDialog("Digite 1 número"));
                num2 = Integer.parseInt(JOptionPane.showInputDialog("Digite 1 número"));
                num3 = Integer.parseInt(JOptionPane.showInputDialog("Digite 1 número"));
 
                if (num1 > num2) {
                    auxiliar = num1;
                    num1 = num2;
                    num2 = auxiliar;
                }
 
                if (num2 > num3) {
                    auxiliar = num2;
                    num2 = num3;
                    num3 = auxiliar;
 
                }
                 
             
                if (num1 > num2) {
                    auxiliar = num1;
                    num1 = num2;
                    num2 = auxiliar;
                }
                 
                 
                 
                JOptionPane.showMessageDialog(null, "Os número em ordem crescente: " + num1 + " " + num2 + " " + num3); 
            }
        }